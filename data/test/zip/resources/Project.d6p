				<IBK:Parameter name="PipeCollectorModelFraction" unit="%">100</IBK:Parameter>
				<Output name="PipeCollectorTr">true</Output>
				<Output name="PipeCollectorQdot">true</Output>
				<CCReference type="Temperature">Tin</CCReference>
				<CCReference type="MassFlowRate">mdot</CCReference>
				<CCReference type="HeatTransferCoefficient">hconv</CCReference>
			</BoundaryCondition>
		</BoundaryConditions>

