	<!--Conditions data, specifies how construction interacts with environment-->
	<Conditions>

		<Interfaces>
			<Interface name="supply_0a" type="Detailed">
				<BCReference>supply_0a_bc</BCReference>
			</Interface>
			<Interface name="return_0a" type="Detailed">
				<BCReference>return_0a_bc</BCReference>
			</Interface>
			<Interface name="supply_0b" type="Detailed">
				<BCReference>supply_0b_bc</BCReference>
			</Interface>
			<Interface name="return_0b" type="Detailed">
				<BCReference>return_0b_bc</BCReference>
			</Interface>
		</Interfaces>

		<ClimateConditions>

			<ClimateCondition name="Tin" type="Temperature" kind="External">
				<IBK:Parameter name="ConstantValue" unit="C">10</IBK:Parameter>
			</ClimateCondition>

			<ClimateCondition name="mdot" type="MassFlowRate" kind="External">
				<IBK:Parameter name="ConstantValue" unit="kg/s">0.1</IBK:Parameter>
			</ClimateCondition>

			<ClimateCondition name="hconv" type="HeatTransferCoefficient" kind="External">
				<IBK:Parameter name="ConstantValue" unit="W/m2K">150</IBK:Parameter>
			</ClimateCondition>
		</ClimateConditions>

		<BoundaryConditions>

			<BoundaryCondition name="return_0a_bc" type="HeatConduction" kind="PipeCollectorModel">
				<IBK:Parameter name="PipeCollectorDiameter" unit="mm">32</IBK:Parameter>
				<IBK:Parameter name="PipeCollectorThickness" unit="mm">3</IBK:Parameter>
				<IBK:Parameter name="PipeCollectorLength" unit="m">120</IBK:Parameter><!-- 120-->
				<IBK:Parameter name="PipeCollectorThermalConductivity" unit="W/mK">0.4</IBK:Parameter>
				<IBK:Parameter name="PipeCollectorModelFraction" unit="%">100</IBK:Parameter>
				<Output name="PipeCollectorTr">true</Output>
				<Output name="PipeCollectorQdot">true</Output>
				<CCReference type="HeatTransferCoefficient">hconv</CCReference>
				<OtherInterfaceReference>supply_0a</OtherInterfaceReference>
			</BoundaryCondition>

			<BoundaryCondition name="return_0b_bc" type="HeatConduction" kind="PipeCollectorModel">
				<IBK:Parameter name="PipeCollectorDiameter" unit="mm">32</IBK:Parameter>
				<IBK:Parameter name="PipeCollectorThickness" unit="mm">3</IBK:Parameter>
				<IBK:Parameter name="PipeCollectorLength" unit="m">120</IBK:Parameter><!-- 120-->
				<IBK:Parameter name="PipeCollectorThermalConductivity" unit="W/mK">0.4</IBK:Parameter>
				<IBK:Parameter name="PipeCollectorModelFraction" unit="%">100</IBK:Parameter>
				<Output name="PipeCollectorTr">true</Output>
				<Output name="PipeCollectorQdot">true</Output>
				<CCReference type="HeatTransferCoefficient">hconv</CCReference>
				<OtherInterfaceReference>supply_0b</OtherInterfaceReference>
			</BoundaryCondition>

			<BoundaryCondition name="supply_0a_bc" type="HeatConduction" kind="PipeCollectorModel">
				<IBK:Parameter name="PipeCollectorDiameter" unit="mm">32</IBK:Parameter>
				<IBK:Parameter name="PipeCollectorThickness" unit="mm">3</IBK:Parameter>
				<IBK:Parameter name="PipeCollectorLength" unit="m">120</IBK:Parameter><!-- 120-->
				<IBK:Parameter name="PipeCollectorThermalConductivity" unit="W/mK">0.4</IBK:Parameter>
				<IBK:Parameter name="PipeCollectorFluidHeatCapacity" unit="J/kgK">4180</IBK:Parameter>
				<IBK:Parameter name="PipeCollectorModelFraction" unit="%">100</IBK:Parameter>
				<Output name="PipeCollectorTr">true</Output>
