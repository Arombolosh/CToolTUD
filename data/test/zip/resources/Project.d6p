				<IBK:Parameter name="PipeCollectorDiameter" unit="mm">32</IBK:Parameter>
				<IBK:Parameter name="PipeCollectorThickness" unit="mm">3</IBK:Parameter>
				<IBK:Parameter name="PipeCollectorLength" unit="m">120</IBK:Parameter><!-- 120-->
				<IBK:Parameter name="PipeCollectorThermalConductivity" unit="W/mK">0.4</IBK:Parameter>
